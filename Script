local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/GemstoneDev/Wally-V3-Backup/main/script/main.lua')))()

local mainWindow = library:CreateWindow("Main")

local mainFolder = mainWindow:CreateFolder("Player")

-- Var --
local lplr = game:GetService("Players").LocalPlayer

mainFolder:Bind("WalkSpeed",Enum.KeyCode.C,function() --Default bind
    print("Run Fast!")
	lplr.Character.Humanoid.WalkSpeed = 75,
	precise = false;
function(v)
end)

mainFolder:Slider("JumpPower",{
	min = 50,
	max = 75,
	precise = false;
},function(v)
		lplr.Character.Humanoid.JumpPower = v
end)

mainFolder:Toggle("Arda Vater",function(bool)
    shared.toggle = bool
    print(shared.toggle)
end)
