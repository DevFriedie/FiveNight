local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local FiveNight = Rayfield:CreateWindow({
	Name = "FiveNight",
	LoadingTitle = "FiveNight",
	LoadingSubtitle = "by DevFriedie",
	ConfigurationSaving = {
	   Enabled = true,
	   FolderName = FiveNight, -- Create a custom folder for your hub/game
	   FileName = "FiveNight Hub"
	},
	Discord = {
	   Enabled = false,
	   Invite = "", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
	   RememberJoins = true -- Set this to false to make them join the discord every time they load it up
	},
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
	   Title = "FiveNight Hub",
	   Subtitle = "",
	   Note = "Discord | Friedie#3095",
	   FileName = "FiveNightKey",
	   SaveKey = true,
	   GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
	   Key = "Awsedrftgz7!"
	}

 })

 Rayfield:Notify({
	Title = "⚠️",
	Content = "Only use Fluxus as executor!",
	Duration = 6.5,
	Actions = { 
	   Ignore = {
		  Name = "Okay!",
		  Callback = function()
		  print("The user tapped Okay!")
	   end
	},
 },
 })

 --Values--
 getgenv().Infinite = true

 --Function--

function Infinite()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end

 local Tab = FiveNight:CreateTab("Main")

 local Section = Tab:CreateSection("")



 local Tab = FiveNight:CreateTab("Other Scripts")

 local Section = Tab:CreateSection("Scripts")

 local Toggle = Tab:CreateToggle({
	Name = "Infinite",
	CurrentValue = false,
	Flag = "", 
	Callback = function()
		Infinite()
	end,
 })

Rayfield:LoadConfiguration()
