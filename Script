if game.PlaceId == 12060450250 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "TEST", HidePremium = true, SaveConfig = true, ConfigFolder = "FiveNight"})

_G.AutoClick = true

function AutoClick
    while _G.AutoClick == true do 
        workspace.DevFriedie.CombatSystem.CombatClient.Punch:FireServer(game:GetService("Players").LocalPlayer)
        wait(.0001)
    end
end

local Player = Window:MakeTab({
	Name = "AutoClick",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Player:AddToggle({
	Name = "This is a toggle!",
	Default = false,
	Callback = function(Value)
		_G.AutoClick = Value
        AutoClick()
	end    
})

end
OrionLib:Init()
